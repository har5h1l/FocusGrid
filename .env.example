# Database connection string
DATABASE_URL=postgres://username:password@hostname:port/database

# Session secret for authentication (generate a random string)
SESSION_SECRET=your_session_secret_here

# OpenRouter API key (required)
# Get your API key from https://openrouter.ai/
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google Calendar API credentials (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:5000/api/calendar/google/callback

# Application URL for redirect URIs
APP_URL=http://localhost:5000 